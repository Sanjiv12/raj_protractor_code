{
  "name": "mstc-bdd-framework",
  "version": "1.0.0",
  "keywords": [
    "protractor",
    "cucumber",
    "typescript",
    "angular",
    "angularjs",
    "testing",
    "behaviour driven development",
    "bdd",
    "selenium",
    "webdriverJS",
    "gherkin",
    "automation testing"
  ],
  "main": "index.js",
  "scripts": {
    "build": "rimraf typeScript/ && tsc",
    "clean": "rimraf typeScript/",
    "package": "zip -rq temp-app.zip dist/newReport_1 && mv temp-app.zip",
    "clean-build": "npm run clean && npm run build",
    "init-contributors": "all-contributors init",
    "add-contributors": "all-contributors add",
    "test" : "protractor typeScript/config/config.js",
    "generate-contributors": "all-contributors generate",
    "bdd": "npm run clean-build && protractor typeScript/config/config.js",
    "webdriver-update": "webdriver-manager update --ignore_ssl",
    "webdriver-start": "webdriver-manager start --ignore_ssl",
    "report": "node typeScript/report/index.js"
  },
  "devDependencies": {
    "@types/cucumber": "^6.0.1",
    "@types/node": "^14.14.10",
    "@types/selenium-webdriver": "^4.0.9",
    "chromedriver": "^99.0.0",
    "cucumber": "^5.1.0",
    "mkdirp": "^1.0.4",
    "multiple-cucumber-html-reporter": "^1.18.0",
    "protractor": "^7.0.0",
    "protractor-cucumber-framework": "^6.2.1",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "@types/chai": "^4.2.14",
    "@types/chai-as-promised": "^7.1.3",
    "@types/cucumber": "^6.0.1",
    "@types/node": "^14.14.10",
    "@types/selenium-webdriver": "^4.0.9",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chromedriver": "^85.0.0",
    "cucumber": "^5.1.0",
    "https-proxy-agent": "^5.0.0",
    "mkdirp": "^1.0.4",
    "multiple-cucumber-html-reporter": "^1.18.0",
    "protractor": "^7.0.0",
    "protractor-cucumber-framework": "^6.2.1",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2"
  }
}
